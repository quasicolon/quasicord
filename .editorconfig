root = true

[*]
charset = utf-8
#end_of_line = crlf
indent_style = tab
insert_final_newline = true
tab_width = 4
ij_formatter_off_tag = @formatter:off
ij_formatter_on_tag = @formatter:on
ij_formatter_tags_enabled = true

[*.xml]
indent_style = space
indent_size = 4

[*.yml]
indent_style = space
indent_size = 2
